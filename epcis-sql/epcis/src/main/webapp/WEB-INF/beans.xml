<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd
http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<jaxws:endpoint id="SoapCaptureService"
		implementor="org.oliot.epcis.service.capture.SoapCaptureService"
		address="/CaptureService">
	</jaxws:endpoint>
	<jaxws:endpoint id="SoapQueryService"
		implementor="org.oliot.epcis.service.query.SoapQueryService" address="/QueryService">
	</jaxws:endpoint>
</beans>